{
	"name": "laravel/laravel",
	"description": "The Laravel Framework.",
	"keywords": ["framework", "laravel"],
	"license": "MIT",
	"require": {
		"laravel/framework": "4.0.*",
		"toddish/verify": "2.1.*",
		"anahkiasen/former": "dev-master",
		"jasonlewis/basset": "dev-master",
		"cviebrock/eloquent-sluggable": "1.0.*",
		"sebastian/money": "1.0.*",
		"intervention/image": "dev-master",
		"natxet/CssMin": "dev-master",
		"lmammino/jsmin4assetic": "1.0.*",
		"nitra/php-min": "dev-master",
		"raveren/kint": "dev-1.0.0-wip",
		"sebklaus/profiler" : "dev-master",
		"aws/aws-sdk-php-laravel": "1.*",
		"patricktalmadge/bootstrapper": "dev-develop",
		"kennedytedesco/validation": "dev-master",
		"tijsverkoyen/css-to-inline-styles": "1.2.*@dev",
		"facebook/php-sdk": "dev-master",
		"goodby/csv": "*",
        "barryvdh/laravel-debugbar": "1.*"
	},
	"autoload": {
		"classmap": [
			"app/libraries",
			"app/commands",
			"app/controllers",
			"app/models",
			"app/helpers",
			"app/exceptions",
			"app/database/migrations",
			"app/database/seeds",
			"app/tests/TestCase.php"
		]
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan optimize"
		],
		"pre-update-cmd": [
			"php artisan clear-compiled"
		],
		"post-update-cmd": [
			"php artisan adjust:assets",
			"php artisan adjust:vendors",
			"php artisan optimize",
            "php artisan debugbar:publish"
		],
		"post-create-project-cmd": [
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "stable"
}
